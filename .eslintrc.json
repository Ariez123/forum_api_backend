{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "google",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": "off",
    "camelcase": "off",
    "require-jsdoc": "off",
    "object-curly-spacing": "off",
    "indent": "off",
    "comma-dangle": "off",
    "operator-linebreak": "off",
    "one-var": "off",
    "no-console": "off",
    "no-undef": "off",
    "no-underscore-dangle": "off",
    "class-methods-use-this": "off",
    "no-unused-vars": "off",
    "max-len": "off",
    "object-curly-newline": "off"
  }
}
